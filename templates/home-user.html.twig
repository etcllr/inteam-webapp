{% extends 'base.html.twig' %}

{% block body %}
    {{ component('navbar') }}
    <br>

    {% for machine in machines %}
        {{ component('card-machine', {
            id: machine.id,
            code: machine.code,
            name: machine.name,
            brand: machine.brand,
            category: machine.category,
            image: machine.image
        }) }}
    {% endfor %}
{% endblock %}